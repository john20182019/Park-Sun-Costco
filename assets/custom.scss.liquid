.product_template {
    .page-content {
        padding-bottom: 0;
        .grid__item {
            @media(max-width: 480px) {
                padding-top: 30px;
                padding-left: 30px;
            }
            .product-single__meta {
                .product-single__title {
                    font-family: Karla, sans-serif;
                    font-style: normal;
                    font-weight: bold;
                    font-size: 24px;
                    line-height: 28px;
                    letter-spacing: 0.28125px;
                    color: #3E2D29;
                    margin-bottom: 15px;
                }
                .product_price {
                    margin: 0;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    .costco_price {
                        width: 30%;
                        font-family: Karla, sans-serif;
                        font-style: normal;
                        font-weight: normal;
                        color: #000000;
                        text-align: left;
                        line-height: 1.2;
                        @media(max-width: 480px) {
                            width: 50%;
                        }
                        .costco_price_title {
                            font-size: 12px;
                            letter-spacing: 0.3px;
                        }
                        .sale-price {
                            font-weight: bold;
                            letter-spacing: 0.3px;
                            font-size: 15px;
                            color: #444;
                        }
                    }
                    .regular_price {
                        width: 70%;
                        font-family: Karla, sans-serif;
                        font-style: normal;
                        font-weight: normal;
                        color: #000;
                        text-align: left;
                        line-height: 1.2;
                        .regular_price_title {
                            font-size: 12px;
                            letter-spacing: 0.3px;
                        }
                        .product__price--compare {
                            font-size: 15px;
                            letter-spacing: 0.3px;
                            font-weight: 400;
                         }
                    }
                }
                .product-special-description {
                    padding-top: 20px;
                    padding-bottom: 5px;
                    p {
                        font-size: 14px;
                        line-height: 22px;
                        font-family: Karla, sans-serif;
                        font-weight: 400;
                        text-transform: capitalize;
                        color: #444444;
                        margin-bottom: 0;
                        @media(max-width: 1024px) {
                            font-size: 12px;
                            line-height: 20px;
                        }
                    }
                }
                .product-short-description {
                    padding-top: 0;
                    padding-bottom: 20px;
                    .speical_des {
                        font-family: Karla, sans-serif;
                        font-weight: bold;
                        font-size: 24px;
                        line-height: 26px;
                        color: #3E2D29;
                        padding-bottom: 10px;
                        letter-spacing: 0.28125px;
                    }
                    .bold-text {
                        font-weight: bold;
                        padding: 10px 0;
                    }
                    p {
                        font-size: 14px;
                        line-height: 22px;
                        font-family: Karla, sans-serif;
                        font-weight: 400;
                        text-transform: capitalize;
                        color: #444444;
                        margin-bottom: 0;
                        @media(max-width: 1024px) {
                            font-size: 12px;
                            line-height: 20px;
                        }
                    }
                }
                .product-single__form {
                    margin-top: 10px;
                    margin-bottom: 25px;
                    .variant-wrapper {
                        margin-bottom: 0;
                        .variant__label {
                            font-size: 16px;
                            line-height: 19px;
                            letter-spacing: 0;
                            font-style: normal;
                            font-weight: bold;
                            text-transform: capitalize;
                            font-family: Karla, sans-serif;
                            margin-bottom: 0;
                        }
                        .variant-input-wrap {
                            margin-bottom: 5px;
                            input[type='radio']:checked+label {
                                padding: 5px 15px;
                                font-size: 16px;
                                line-height: 19px;
                                background: #00a7e1;
                                color: #fff;
                                box-shadow: none;
                                border-radius: 4px;
                            }
                            label {
                                box-shadow: none;
                            }
                        }
                    }
                    .product__quantity {
                        display: flex;
                        align-items: center;
                        margin-bottom: 10px;
                        label {
                            text-transform: capitalize;
                            font-style: normal;
                            font-weight: 400;
                            font-size: 14px;
                            letter-spacing: 0.3px;
                            font-family: Karla, sans-serif;
                            margin-bottom: 0;
                            margin-right: 10px;
                            line-height: 1.8;
                            @media(max-width: 1024px) {
                                font-size: 12px;
                            }
                        }
                        .js-qty__wrapper {
                            width: 66px;
                            min-width: 66px;
                            border: 0.5px solid #666666;
                            box-sizing: border-box;
                            border-radius: 2px;
                            .js-qty__num {
                                border: none;
                                padding: 0;
                                font-size: 14px;
                                font-family: karla sans-serif;
                                line-height: 1.5;
                                @media(max-width: 1024px) {
                                    font-size: 12px;
                                }
                            }
                            .js-qty__adjust--minus{
                                background-color: #fff !important;
                                border: none;
                                border-right: 1px solid #666;
                                padding: 0 5px;
                            }
                            .js-qty__adjust--plus {
                                background-color: #fff !important;
                                border: none;
                                border-left: 1px solid #666;
                                padding: 0 5px;
                            }
                        }
                    }
                    .payment-buttons {
                        margin-top: 20px;
                        .btn--tertiary {
                            font-size: 14px;
                            line-height: 16px;
                            letter-spacing: 0.5px;
                            text-transform: uppercase;
                            max-width: 358px;
                            background: #02297A;
                            border-radius: 2px !important;
                            padding: 5px 20px;
                            border: none;
                            min-height: 45px;
                            width: 100%;
                            color: #fff;
                            font-family: Karla, sans-serif;
                            font-style: normal;
                            font-weight: 400;
                        }
                        .add-to-cart-disclaimer {
                            display: none;
                        }
                    }
                }
