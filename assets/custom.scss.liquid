@font-face {
    font-family: "ProximaNova-Regular";
    src: url("./fonts/ProximaNova-Regular.eot"); /* IE9 Compat Modes */
    src: url("./fonts/ProximaNova-Regular.eot?#iefix") format("embedded-opentype"), /* IE6-IE8 */
      url("./fonts/ProximaNova-Regular.otf") format("opentype"), /* Open Type Font */
      url("./fonts/ProximaNova-Regular.svg") format("svg"), /* Legacy iOS */
      url("./fonts/ProximaNova-Regular.ttf") format("truetype"), /* Safari, Android, iOS */
      url("./fonts/ProximaNova-Regular.woff") format("woff"), /* Modern Browsers */
      url("./fonts/ProximaNova-Regular.woff2") format("woff2"); /* Modern Browsers */
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: "Karla-Regular";
    src: url("./fonts/Karla-Regular.eot"); /* IE9 Compat Modes */
    src: url("./fonts/Karla-Regular.eot?#iefix") format("embedded-opentype"), /* IE6-IE8 */
      url("./fonts/Karla-Regular.otf") format("opentype"), /* Open Type Font */
      url("./fonts/Karla-Regular.svg") format("svg"), /* Legacy iOS */
      url("./fonts/Karla-Regular.ttf") format("truetype"), /* Safari, Android, iOS */
      url("./fonts/Karla-Regular.woff") format("woff"), /* Modern Browsers */
      url("./fonts/Karla-Regular.woff2") format("woff2"); /* Modern Browsers */
    font-weight: normal;
    font-style: normal;
}

.transition-body .main-content {
    background-color: #eef2f6d4;
    .shopify-section.index-section {
        margin-bottom: 0;
        padding-bottom: 34px;
        @media(max-width: 1024px) {
            padding-bottom: 0;
        }
    }
    #shopify-section-product-template {
        padding: 20px 30px;
        .breadcrumb {
            max-width: 1300px;
            margin: 0 auto;
            padding: 20px 15px;
            padding-top: 0;
        }
        .page-content {
            background: #fff;
            padding-top: 0;
            .grid {
                padding-top: 20px;
                .medium-up--two-fifths {
                    padding-left: 0;
                }
            }
        }
    }
}
.header-wrapper.payment-icon {
    position: relative;
    .header-item {
        position: absolute;
        left: 5%;
        top: 0;
        z-index: 99;
        .site-nav {
            .site-nav__link {
                color: #fff;
                padding-top: 5px;
                .icon {
                    @media(max-width: 480px) {
                        width: 20px;
                    } 
                }
            }
        }
    }
    .announcement {
        background: #007db8;
        .announcement__text {
            padding: 12px 10px;
            font-family: Karla, sans-serif;
            font-style: normal;
            font-weight: normal;
            font-size: 16px;
            line-height: 18px;
            text-align: center;
            letter-spacing: 0.2625px;
            @media(max-width: 1024px) {
                font-size: 14px;
                line-height: 16px;
            }
            @media(max-width: 480px) {
                font-size: 12px;
            }
        }
    }
    .site-nav--icons {
        position: absolute;
        top: 0;
        right: 25px;
        width: 5%;
        height: 40px;
        @media(max-width: 480px) {
            right: 45px;
        }
        .site-nav__icons {
            .site-nav__link {
                padding-right: 5px;
                @media(max-width: 480px) {
                    right: 3px;
                }
                .cart-link {
                    z-index: 29;
                    color: #fff;
                    display: flex;
                    align-items: center;
                    .icon {
                        display: none;
                        @media(max-width: 480px) {
                            width: 22px;
                        } 
                    }
                    .icon__fallback-text {
                        overflow: visible;
                        font-size: 14px;
                        width: 100%;
                        color: #fff;
                        height: 100%;
                        position: relative;
                        font-weight: 100;
                         
                    }
                    .cart-link__bubble {
                        display: block;
                        font-weight: 100;
                        top: 0px;
                        right: -13px;
                        background: none;
                    }
                }
            }
            .american-flag {
                z-index: 29;
                width: 22px;
                height: 14px;
            }
            
        }
    }
    .site-header {
        .icon-bar-background {
            display: none;
        }
    }
}

.header-logos-bar {
    display: flex;
    align-items: center;
    .site-nav {
        .site-nav__item {
            padding-bottom: 6px;
            .site-nav__link {
                font-size: 16px;
            }
        }
    }
    .costco-logo {
        position: absolute;
        top: 0;
        bottom: 0;
        right: 30px;
        @media(max-width: 768px) {
            margin-right: 25px;
        }
        img {
            padding-top: 10%;
            max-width: 168px;
            width: 100%;
            @media(max-width: 1024px) {
                width: 168px;
            }
        }
    }
}

.background-media-text__inner {
    .background-media-text__aligner {
        margin-top: 10%;
        margin-left: 1%;
       @media(max-width: 589px) {
            margin: 0;
            text-align: center;
        }
        .animation-contents {
            .rte.background-media-text__subtext {
                font-family: Karla, sans-serif;
                font-style: normal;
                font-weight: bold;
                font-size: 32px;
                line-height: 38px;
                letter-spacing: 1px;
                color: #FFFFFF;
                text-shadow: 0px 0px 5px rgba(0, 0, 0, 0.25);
                @media(max-width: 1024px) {
                    font-size: 26px;
                    line-height: 30px;
                    letter-spacing: 0.4875px;
                }
                p {
                    max-width: 320px;
                    @media(max-width: 1024px) {
                        max-width: 310px;
                    }
                    @media(max-width: 589px) {
                        max-width: 100%;
                        text-align: center;
                        color: #1c1d1dc2;
                    }
                }
            }
        }
    }
}

.featured-collection {
    .section-header {
        .section-header__title {
            font-family: Karla, sans-serif;
            font-style: normal;
            font-weight: bold;
            font-size: 20px;
            line-height: 24px;
            letter-spacing: 1px;
