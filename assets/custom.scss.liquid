                            font-style: normal;
                            font-weight: normal;
                            color: #777677;
                            text-align: left;
                            line-height: 1.2;
                            .regular_price_title {
                                font-size: 12px;
                                line-height: 14px;
                                letter-spacing: 0.3px;
                            }
                            .grid-product__price--original {
                                font-size: 14px;
                                line-height: 16px;
                                letter-spacing: 0.3px;
                            }
                        }
                    }
                }
            }
        }
    }
}
